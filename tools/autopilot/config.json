{
  "bot": {
    "git_name": "onetoo-autopilot-bot",
    "git_email": "autopilot-bot@onetoo.eu",
    "commit_message": "autopilot: sync registries [autopilot]"
  },
  "allowlist": [
    "public/dumps/contrib-accepted.json",
    "public/dumps/contrib-sandbox.json",
    "public/dumps/contrib-autopilot.json",
