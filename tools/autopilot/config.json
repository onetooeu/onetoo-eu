    "public/dumps/autopilot-decisions.json",
    "public/dumps/ai-search-index.json"
  ],
  "canonical_json": {
    "sort_keys": true,
    "compact": true,
    "ensure_ascii": false,
    "newline": true
  },
  "rules": {
    "sort_items_by": ["id", "domain", "url"],
    "max_items": 200000
  }
}
